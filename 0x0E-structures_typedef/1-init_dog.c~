#include <stdio.h>


struct dog {
    char *name;
    float age;
    char *owner;
};

/**
 * init_dog - initializes dog
 * @d: first variable
 * @name: second variable
 * @age: third variable
 * @owner: fourth variable
 */


void init_dog(struct dog *d, char *name, float age, char *owner) {
    d->name = name;
    d->age = age;
    d->owner = owner;
}

